{
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "vim.useCtrlKeys": false,
    "breadcrumbs.enabled": true,
    "window.zoomLevel": 0,
    "[javascript]": {
        
    },
    "editor.fontFamily": "'FuraMono Nerd Font', 'FuraMono', 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "debug.console.fontFamily": "FuraMono Nerd Font",
